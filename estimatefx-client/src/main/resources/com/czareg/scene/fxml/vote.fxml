<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.czareg.controller.VoteController">
   <padding>
      <Insets bottom="20.0" left="40.0" right="40.0" top="10.0" />
   </padding>
   <children>
      <HBox spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="userStatusLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="200.0" text="User Status" />
            <Button fx:id="stealCreatorButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleStealCreatorButtonClicked" text="Steal Creator" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <HBox spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="sessionStatusLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="200.0" text="Session Status" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleLeaveButtonClicked" text="Leave session" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Label fx:id="usersStatusLabel" layoutX="50.0" layoutY="47.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="200.0" text="Users Status" />
      <Separator layoutX="50.0" layoutY="109.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <HBox spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="votingStatusLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="200.0" text="Voting Status" HBox.hgrow="ALWAYS" />
            <Button fx:id="startButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleStartButtonClicked" text="Start" HBox.hgrow="ALWAYS" />
            <Button fx:id="stopButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleStopButtonClicked" text="Stop" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox fx:id="buttonsHBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="15.0" VBox.vgrow="ALWAYS">
         <children>
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVoteButtonClicked" text="?" HBox.hgrow="ALWAYS" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVoteButtonClicked" text="0" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVoteButtonClicked" text="1" HBox.hgrow="ALWAYS" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVoteButtonClicked" text="2" HBox.hgrow="ALWAYS" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVoteButtonClicked" text="3" HBox.hgrow="ALWAYS" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVoteButtonClicked" text="5" HBox.hgrow="ALWAYS" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVoteButtonClicked" text="8" HBox.hgrow="ALWAYS" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVoteButtonClicked" text="13" HBox.hgrow="ALWAYS" />
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVoteButtonClicked" text="21" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <TableView fx:id="voteTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="250.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="nameColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="30.0" prefWidth="220.0" text="Name" />
            <TableColumn fx:id="voteColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="30.0" text="Vote" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <contextMenu>
            <ContextMenu>
               <items>
                  <MenuItem fx:id="passCreatorButton" mnemonicParsing="false" onAction="#handlePassCreatorButtonClicked" text="Pass Creator" />
                  <MenuItem fx:id="kickUserButton" mnemonicParsing="false" onAction="#handleKickUserButtonClicked" text="Kick User" />
               </items>
            </ContextMenu>
         </contextMenu>
      </TableView>
   </children>
</VBox>
